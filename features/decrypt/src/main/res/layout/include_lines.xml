<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/view_line1"
        android:layout_width="0dp"
        android:layout_height="@dimen/plu_size_03"
        android:layout_marginTop="@dimen/plu_size_02"
        android:background="@drawable/plu_bg_rounded_corners_solid"
        app:layout_constraintBottom_toTopOf="@id/view_line2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_line2"
        android:layout_width="0dp"
        android:layout_height="@dimen/plu_size_03"
        android:layout_marginTop="@dimen/plu_size_02"
        android:background="@drawable/plu_bg_rounded_corners_solid"
        app:layout_constraintBottom_toTopOf="@id/view_line3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_line1" />

    <View
        android:id="@+id/view_line3"
        android:layout_width="0dp"
        android:layout_height="@dimen/plu_size_03"
        android:layout_marginTop="@dimen/plu_size_02"
        android:background="@drawable/plu_bg_rounded_corners_solid"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_line2" />

</androidx.constraintlayout.widget.ConstraintLayout>
